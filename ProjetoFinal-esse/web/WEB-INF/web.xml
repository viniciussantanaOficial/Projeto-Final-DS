<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>CadastroController</servlet-name>
        <servlet-class>controller.CadastroController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tela_inicialController</servlet-name>
        <servlet-class>controller.Tela_inicialController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>controller.HomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarrinhoController</servlet-name>
        <servlet-class>controller.CarrinhoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pre_carrinhoController</servlet-name>
        <servlet-class>controller.pre_carrinhoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PerfilController</servlet-name>
        <servlet-class>controller.PerfilController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TelaAdmController</servlet-name>
        <servlet-class>controller.TelaAdmController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tela_VinhoController</servlet-name>
        <servlet-class>controller.Tela_VinhoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tela_PerfumesController</servlet-name>
        <servlet-class>controller.Tela_PerfumesController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tela_Vinhos2Controller</servlet-name>
        <servlet-class>controller.Tela_Vinhos2Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Tela_EletronicosController</servlet-name>
        <servlet-class>controller.Tela_EletronicosController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModeloController</servlet-name>
        <servlet-class>controller.ModeloController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckoutController</servlet-name>
        <servlet-class>controller.CheckoutController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroController</servlet-name>
        <url-pattern>/cadastro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/logar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tela_inicialController</servlet-name>
        <url-pattern>/tela_inicial</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarrinhoController</servlet-name>
        <url-pattern>/carrinho</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pre_carrinhoController</servlet-name>
        <url-pattern>/pre_carrinho</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastroController</servlet-name>
        <url-pattern>/cadastrar-usuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PerfilController</servlet-name>
        <url-pattern>/perfil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TelaAdmController</servlet-name>
        <url-pattern>/telaadm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TelaAdmController</servlet-name>
        <url-pattern>/cadastrarProduto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tela_VinhoController</servlet-name>
        <url-pattern>/TelaVinhos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tela_PerfumesController</servlet-name>
        <url-pattern>/Tela_Perfumes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tela_Vinhos2Controller</servlet-name>
        <url-pattern>/Tela_Vinhos2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Tela_EletronicosController</servlet-name>
        <url-pattern>/Tela_Eletronicos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModeloController</servlet-name>
        <url-pattern>/modelo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckoutController</servlet-name>
        <url-pattern>/checkout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>redirect.jsp</welcome-file>
    </welcome-file-list>
</web-app>
